function setBinData(h,bindata)

%  setBinData(h,bindata)
%
% Associates the current bindata with current data set
%
% bindata is a structure returned by m
%
% See also makeBinData



if nargin < 1 || isempty(h) || ~ishandle(h)    
    h = GazeReader;
end

ehd = getappdata(h,'eyetrackerHeaderData');

if nargin < 2
    dataindices = 1:length(ehd);
end


dsfun = getappdata(h,'DataSetWindowFunctions');

dsfun.pool(dataindices);


